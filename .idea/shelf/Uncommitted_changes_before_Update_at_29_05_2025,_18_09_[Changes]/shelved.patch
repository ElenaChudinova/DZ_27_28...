+        auto_now_add=True, verbose_name=" ", editable=False, blank=True
+    )
+    updated_at = models.DateTimeField(
+        auto_now=True,
+        verbose_name="  ",
+        editable=False,
+        blank=True,
+    )
+    category = models.ForeignKey(
